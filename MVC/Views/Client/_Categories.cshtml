@model List<MVC.Models.Shared.CategoryViewModel>

<table>

    @if (Model != null)
    {
        for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>
                    @Html.ActionLink(Model[i].CategoryName + " " + Model[i].TestsCount, "ViewCategory", new { categoryId = Model[i].Id })
                </td>
            </tr>
        }
    }

</table>